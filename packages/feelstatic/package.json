{
  "name": "feelstatic",
  "version": "0.1.7",
  "description": "Edit content and fields for static sites in Next.js",
  "author": "Mikkel Damm <mikkeldamm@hotmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./api": "./dist/api/index.js",
    "./field": "./dist/components/field.js",
    "./middleware": "./dist/middleware/index.js",
    "./feelstatic.css": "./dist/feelstatic.css"
  },
  "files": [
    "dist",
    "field.d.ts",
    "api.d.ts",
    "middleware.d.ts",
    "types.d.ts",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "dev": "pnpm run dev:styles && pnpm run build:types && pnpm run dev:files",
    "dev:styles": "tailwindcss -i ./src/feelstatic.css -o ./dist/feelstatic.css && tailwindcss -i ./src/feelstatic.css -o ./dist/feelstatic.css --watch",
    "dev:files": "swc src -d dist --watch",
    "build": "pnpm run clean && pnpm run build:styles && pnpm run build:types && pnpm run build:files",
    "build:styles": "tailwindcss ./src/feelstatic.css -o ./dist/feelstatic.css",
    "build:files": "swc src -d dist",
    "build:types": "tsc",
    "clean": "rm -rf dist",
    "test": "jest --passWithNoTests",
    "check": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint \"src/**/*.ts*\""
  },
  "dependencies": {
    "@dnd-kit/core": "6.0.7",
    "@dnd-kit/sortable": "7.0.2",
    "@dnd-kit/utilities": "3.2.1",
    "@octokit/rest": "19.0.5",
    "encoding": "0.1.13",
    "jotai": "1.13.0",
    "js-base64": "3.7.4",
    "octokit-commit-multiple-files": "4.2.0",
    "react-icons": "4.7.1",
    "swr": "2.0.0"
  },
  "devDependencies": {
    "@swc/cli": "0.1.59",
    "@swc/core": "1.3.26",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "10.4.13",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.2",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "next": "13.1.2",
    "prettier": "2.8.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.2.4",
    "postcss": "8.4.21",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "next": "^13",
    "react": "^18",
    "react-dom": "^18"
  }
}
